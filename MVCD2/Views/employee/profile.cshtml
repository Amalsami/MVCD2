@{
    <html>
    <head>
        <title>Employees</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    </head>
    <body>
        <h2>data about employee @Model.FirstName </h2>


        <div class="card mb-2 " style="width: 18rem">
            <ul>
                <li>FName: @Model.FirstName</li>
                <li>MidName: @Model.MiddleName</li>
                <li>Lname: @Model.LastName</li>
                <li>Address: @Model.Address</li>
                <li>Birthdate: @Model.BirthDate</li>
                <li>Gender: @Model.Sex</li>
                <li>Salary: @Model.Salary</li>

            </ul>
        </div>


        <a href="/Dependance/Add">Add Dependence</a>
        <a href="/Dependance/index">Get all Dependence</a>

    </body>
    </html>
}
